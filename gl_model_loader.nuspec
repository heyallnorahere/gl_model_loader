<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>gl_model_loader</id>
    <version>1.6.2</version>
    <description>
      please report any issues to the github repo
      disclaimer: this library uses glew to load opengl functions
      if you are using a loader like glad make sure to initialize glew or switch to it
    </description>
    <tags>Native native OpenGL opengl Opengl dynamic Dynamic</tags>
    <projectUrl>https://github.com/yodasoda1219/gl_model_loader</projectUrl>
    <license type="file">metadata/LICENSE</license>
    <authors>Theo Beda</authors>
    <dependencies>
        <dependency id="assimp.v140" version="3.2.0"/>
        <dependency id="glm" version="0.9.9.800"/>
        <dependency id="unofficial-flayan-glew" version="2.1.0"/>
    </dependencies>
  </metadata>
  <files>
    <file src="x64\Release\model_loader.lib" target="build/native/lib/x64/Release"/>
    <file src="x64\Release\model_loader.dll" target="build/native/bin/x64/Release"/>
    <file src="x64\Debug\model_loader.lib" target="build/native/lib/x64/Debug"/>
    <file src="x64\Debug\model_loader.dll" target="build/native/bin/x64/Debug"/>
    <file src="*.h" target="build/native/include"/>
    <file src="gl_model_loader.props" target="build/native"/>
    <file src="LICENSE" target="metadata/LICENSE"/>
  </files>
</package>